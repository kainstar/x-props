{
  "name": "x-props",
  "version": "1.0.0",
  "description": "A expression props evaluate component generator",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.tsx --format=cjs,esm --dts",
    "cm": "cz",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kainstar/x-props.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kainstar",
    "email": "15249633+kainstar@users.noreply.github.com",
    "url": "https://github.com/kainstar"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "expression",
    "props",
    "x-props",
    "react",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/kainstar/x-props/issues"
  },
  "homepage": "https://github.com/kainstar/x-props#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/react": "^14.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "@types/traverse": "^0.6.32",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.0",
    "lodash.mergewith": "^4.6.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "deepmerge": "^4.3.0",
    "traverse": "^0.6.7"
  }
}
